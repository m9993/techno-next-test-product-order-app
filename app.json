    "icon": "src/assets/images/branding/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "src/assets/images/branding/splash.png",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1",
