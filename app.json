{
  "expo": {
    "name": "Product Order",
    "slug": "product-order",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/images/branding/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "src/assets/images/branding/splash.png",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1",
      "bundleIdentifier": "com.product.order",
      "icon": "src/assets/images/branding/icon.png",
      "config": {
        "googleMapsApiKey": ""
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/images/branding/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.product.order",
      "versionCode": 1,
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      }
    },
    "androidStatusBar": {
      "hidden": true,
      "translucent": true
    },
    "web": {
      "favicon": "src/assets/images/branding/favicon.png"
    },
    "plugins": [
      ["expo-localization", {}],
      "expo-font",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ]
  }
}
